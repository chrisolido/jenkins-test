pipeline {
    agent any
    stages {

        stage('Step 1 pipeline'){
          steps{
		    echo 'test1'
                sh 'mkdir from-jenkins'
                sh 'touch from-jenkins/test.txt'
                }
        }
        }
    stages {

        stage('Step 2  pipeline'){
          steps{
		    echo 'test1'
                sh 'mkdir from-jenkins'
                sh 'touch from-jenkins/test.txt'
                }
        }
        }
        stages {

        stage('Step 3  pipeline'){
          steps{
		    echo 'test1'
                sh 'mkdir from-jenkins'
                sh 'touch from-jenkins/test.txt'
                }
        }
        }
}
}
